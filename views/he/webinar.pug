extends base 

block head
    title וובינר השקעות נדל\"ן בבולגריה
    meta(name="description" content="אתם מוזמנים לוובינר השקעות הנדל\"ן הגדול בבולגריה של דירה בולגרית")
    meta(name="robots" content="index, follow")
    link(rel="canonical" href="https://www.dirabulgarit.com/webinar")

    meta(property="og:title" content="וובינר השקעות נדל\"ן בבולגריה")
    meta(property="og:description" content="אתם מוזמנים לוובינר השקעות הנדל\"ן הגדול בבולגריה של דירה בולגרית")
    meta(property="og:image" content='https://www.dirabulgarit.com/img/webinar-link-img.webp')
    meta(property="og:image:width" content="1200")
    meta(property="og:image:height" content="630")
    include _headLinksScripts

block content 

    .container-fluid.webinar-main-container
        .row.webinar-headline-row
            .col.webinar-headline-col 
                img.webinar-headline-img(src='/img/webinar-image.jpg' alt='תמונת נוף של סופיה') 
                h1.webinar-headline דירה בולגרית שמחה להזמין אתכם לוובינר השקעות הנדל"ן המקיף בבולגריה
                h2.webinar-headline-text-top(dir='rtl') יום חמישי | 7.11.24
                h2.webinar-headline-text-bottom(dir='rtl') 20:30 | zoom         

    .container-fluid.webinar-contact-us-top-container
        .row.landing-contact-us-headline-row.d-felx.justify-content-center
            .col-12.col-md-10.col-xl-8.landing-contact-us-headline-col
